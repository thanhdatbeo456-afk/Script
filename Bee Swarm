local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/thanhdatbeo456-afk/Script/refs/heads/main/Redz"))()

redzlib:MakeLoading({
Title = "Bee Swarm Simulator | v0.1",
Duration = 5
})
redzlib:MakeNotification({
Title = "Alert",
Description = "Loaded",
Duration = 3
})
local Window = redzlib:MakeWindow({
  Title = "Bee Swarm Simulator",
  SubTitle = "v0.1",
  SaveFolder = "Redz | redz lib v5.lua"
})

Window:AddMinimizeButton({
    Button = { Image = "", BackgroundTransparency = 0 },
    Corner = { CornerRadius = UDim.new(0, 5) },
})

local Tabs = {
Main = Window:MakeTab({"Main", "home"}),
Shop = Window:MakeTab({"Shop", "coin"}),
Teleport = Window:MakeTab({"Teleport", "compass"}),
Setting = Window:MakeTab({"Setting", "settings"})
}
Tabs.Main:AddSection({"Farm"})
local function GetChar() return game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait() end
game:GetService("RunService").Stepped:Connect(function()
if _G.AutoFarm then
local char = GetChar()
local root = char:FindFirstChild("HumanoidRootPart")
local hum = char:FindFirstChild("Humanoid")
local field = workspace.FlowerZones:FindFirstChild(_G.SelectedField or "Dandelion Field")
if field and root then
local monsterNearby = false
for _, monster in ipairs(workspace.Monsters:GetChildren()) do
if monster:FindFirstChild("HumanoidRootPart") then
local dist = (monster.HumanoidRootPart.Position - root.Position).Magnitude
if dist < 15 then
monsterNearby = true
hum.Jump = true
root.CFrame = monster.HumanoidRootPart.CFrame * CFrame.new(0, 5, 7) -- Đứng sau và cao hơn quái
task.wait(0.2)
end
end
end
local tokenFound = false
if _G.CollectTokens then
for _, token in ipairs(workspace.Collectibles:GetChildren()) do
local tDist = (token.Position - field.Position).Magnitude
if tDist < field.Size.X/2 and token.Transparency < 1 then
tokenFound = true
hum:MoveTo(token.Position)
break 
end
end
end
if not tokenFound and not monsterNearby then
local randomPos = field.Position + Vector3.new(math.random(-field.Size.X/3, field.Size.X/3), 0, math.random(-field.Size.Z/3, field.Size.Z/3))
hum:MoveTo(randomPos)
end
if _G.AutoDig then
game:GetService("ReplicatedStorage").Events.ToolCollect:FireServer()
end
if _G.AutoSprinkler then
game:GetService("ReplicatedStorage").Events.PlayerAbilityEvent:FireServer("Sprinkler")
end
end
end
end
end)
local Fields = {"Dandelion Field", "Sunflower Field", "Mushroom Field", "Blue Flower Field", "Clover Field", "Strawberry Field", "Spider Field", "Bamboo Field", "Pineapple Patch", "Cactus Field", "Pumpkin Patch", "Pine Tree Forest", "Rose Field", "Mountain Top Field", "Coconut Patch", "Pepper Patch"}
Tabs.Main:AddDropdown({
  Name = "Select Field",
  Options = {Fields},
  Default = "",
  Callback = function(v)
_G.SelectFieldFarm = v
end})
Tabs.Main:AddCheckbox({
  Name = "Auto Farm",
  Default = false,
  Callback = function(v)
_G.AutoFarm = v
end})
Tabs.Main:AddCheckbox({
  Name = "Auto Collect Token",
  Default = false,
  Callback = function(v)
_G.CollectTokens = v
end})
Tabs.Main:AddCheckbox({
  Name = "Auto Sprinker",
  Default = false,
  Callback = function(v)
_G.AutoSprinkler = v
end})
Tabs.Main:AddCheckbox({
  Name = "Auto Dig",
  Default = false,
  Callback = function(v)
_G.AutoDig = v
end})
Tabs.Main:AddSection({"Collectibles"})
Tabs.Main:AddSlider({
  Name = "Delay Collectibles",
  Min = 0.3,
  Max = 1,
  Increase = 0.1,
  Default = 0.3,
  Callback = function(v)
_G.DelayCollectibles = v
end})
local AutoAllCollectibles = Tabs.Main:AddCheckbox({
  Name = "Auto All Collectibles",
  Default = false,
  Callback = function(v)
_G.AutoAllCollectibles = v
AutoToolCollect:Set(false)
while _G.AutoCollectibles do
for _,v in ipairs(game:GetService("Workspace").Collectibles:GetChildren()) do
if v.Parent.Name == "Collectibles" and not v.Transparency => 0.7 then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame - Vector3.new(0,0,0)
end
end
wait(1)
end
end})
